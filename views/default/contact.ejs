<%- include('../partials/default/header'); -%>


  <main class="site-main page-spacing">
    <!-- Page Banner -->
    <div class="page-banner container-fluid no-padding">
      <div class="page-banner-content">
        <h3>Contact</h3>
      </div>
    </div>
    <!-- Page Banner /- -->

    <!-- Contact Us 1 -->
    <div class="contact-us-1 container-fluid no-padding">
      <div class="section-padding"></div>
      <!-- Container -->
      <div class="container">
        <div class="col-md-6 col-sm-6 col-xs-6">
          <!-- Section Header -->
          <div class="section-header left-header">
            <h3>Get In Touch</h3>
            <img src="/assets/images/section-seprator-1.png" alt="section-seprator-1" width="70" height="3" />
            <p>
              contact us monday's to saturday for extra queries, sales support, and you will recieve an immediate
              response
            </p>
          </div>
          <!-- Section Header /- -->
          <form class="row">
            <div class="form-group col-md-6">
              <input type="text" id="input_email" name="contact-email" required="" placeholder="Your Email"
                class="form-control" />
            </div>
            <div class="form-group col-md-6">
              <input type="text" id="input_phone" required="" name="contact-phone" placeholder="Your Phone Number"
                class="form-control" />
            </div>

            <div class="form-group col-md-12">
              <textarea id="textarea_message" name="contact-message" placeholder="Your Message" rows="6"
                class="form-control"></textarea>
            </div>
            <input type="submit" title="Send Now" value="Send Now" id="btn_submit" name="Send Now" />
            <div id="alert-msg" class="alert-msg"></div>
          </form>
        </div>

        <div class="col-md-6 col-sm-6 col-xs-6 contact-detail">
          <!-- Map Section -->
          <div class="map">
            <div class="map-canvas" id="map-canvas-contact" data-lat="5.79565" data-lng="7.03513"
              data-string="orlu, imo state, Nigeria" data-zoom="12"></div>
          </div>
          <!--  Map Section /- -->
        </div>
      </div>
      <!-- Container /- -->
      <div class="section-padding"></div>
    </div>
    <!-- Contact Us 1 /- -->


    <%- include('../partials/default/footer'); -%>